{
	"name": "filen-desktop",
	"author": "Filen Cloud Dienste UG (haftungsbeschränkt) <support@filen.io>",
	"description": "Filen Desktop Client",
	"version": "2.0.6",
	"private": true,
	"engines": {
		"node": "14.x"
	},
	"license": "AGPL-3.0",
	"repository": {
		"url": "https://github.com/FilenCloudDienste/filen-desktop"
	},
	"dependencies": {
		"@chakra-ui/react": "^1.8.7",
		"@emotion/react": "^11.8.2",
		"@emotion/styled": "^11.8.1",
		"@gerhobbelt/gitignore-parser": "^0.2.0-9",
		"@sentry/react": "^7.17.4",
		"@sentry/tracing": "^7.17.4",
		"@uiw/react-codemirror": "^4.7.0",
		"auto-launch": "^5.0.5",
		"crypto-js": "^4.1.1",
		"electron-disable-file-drop": "^1.0.2",
		"electron-is": "^3.0.0",
		"electron-log": "^4.4.8",
		"electron-traywindow-positioner": "^1.1.1",
		"electron-updater": "^5.2.1",
		"framer-motion": "^3.10.6",
		"fs-extra": "^10.1.0",
		"js-base64": "^3.7.2",
		"js-md2": "^0.2.2",
		"js-md4": "^0.3.2",
		"js-md5": "^0.7.3",
		"js-sha1": "^0.6.0",
		"js-sha256": "^0.9.0",
		"js-sha512": "^0.8.0",
		"lodash": "^4.17.21",
		"mime-types": "^2.1.35",
		"node-watch": "github:FilenCloudDienste/node-watch-fs-extra",
		"progress-stream": "^2.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-fast-compare": "^3.2.0",
		"react-icons": "^4.3.1",
		"react-router-dom": "^6.3.0",
		"react-virtualized": "^9.22.3",
		"readdirp": "^3.6.0",
		"request": "^2.88.2",
		"socket.io-client": "^2.4.0",
		"speed-limiter": "^1.0.2",
		"striptags": "^3.2.0",
		"uuid": "^8.3.2",
		"write-file-atomic": "^4.0.1"
	},
	"scripts": {
		"start": "concurrently -k \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electronmon .\"",
		"dev": "concurrently -k \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electronmon .\"",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"dist": "rm -rf dist/ && react-scripts build && electron-builder -wlm --publish never",
		"build-linux": "rm -rf dist/ && react-scripts build && electron-builder -l --publish never",
		"build-mac": "rm -rf dist/ && react-scripts build && electron-builder -m --publish never",
		"build-win": "rm -rf dist/ && react-scripts build && electron-builder -w --publish never",
		"build-win-win": "react-scripts build && electron-builder -w --publish never",
		"react-start": "react-scripts start",
		"rebuild": "electron-rebuild",
		"rebuild:arm64": "electron-rebuild --arch arm64",
		"postinstall": "electron-builder install-app-deps",
		"icons": "electron-icon-builder --input=./assets/icon.png --output=./assets/"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^28.1.6",
		"@types/node": "^18.7.2",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@types/request": "^2.48.8",
		"@types/uuid": "^8.3.4",
		"concurrently": "^5.3.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.1",
		"electron": "^20.0.2",
		"electron-builder": "^23.0.3",
		"electron-icon-builder": "^2.0.1",
		"electron-notarize": "^1.2.1",
		"electronmon": "^1.1.2",
		"react-scripts": "^5.0.1",
		"typescript": "^4.7.4",
		"wait-on": "^5.3.0"
	},
	"homepage": "./",
	"main": "./src/main/index.js",
	"build": {
		"appId": "io.filen.desktop",
		"productName": "Filen",
		"asar": false,
		"npmRebuild": false,
		"nodeGypRebuild": false,
		"artifactName": "filen_${arch}.${ext}",
		"afterSign": "./scripts/notarize.js",
		"extends": null,
		"files": [
			"build",
			"node_modules",
			"src",
			"package.json",
			"assets"
		],
		"directories": {
			"output": "./dist/",
			"buildResources": "./assets/"
		},
		"publish": [
			{
				"provider": "generic",
				"url": "https://cdn.filen.io/desktop/release/"
			}
		],
		"win": {
			"publisherName": [
				"Filen Cloud Dienste UG (haftungsbeschränkt)"
			],
			"certificateSubjectName": "Filen Cloud Dienste UG (haftungsbeschränkt)",
			"verifyUpdateCodeSignature": false,
			"icon": "./assets/icons/win/icon.ico",
			"target": {
				"target": "nsis",
				"arch": [
					"x64",
					"arm64"
				]
			},
			"publish": [
				{
					"provider": "generic",
					"url": "https://cdn.filen.io/desktop/release/"
				}
			],
			"sign": "./scripts/sign.js"
		},
		"nsis": {
			"oneClick": false,
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true,
			"deleteAppDataOnUninstall": true,
			"allowToChangeInstallationDirectory": false
		},
		"linux": {
			"maintainer": "Filen Cloud Dienste UG (haftungsbeschränkt)",
			"vendor": "Filen Cloud Dienste UG (haftungsbeschränkt)",
			"category": "Utility",
			"icon": "./assets/icons/png/",
			"target": {
				"target": "AppImage",
				"arch": [
					"x64",
					"arm64"
				]
			},
			"publish": [
				{
					"provider": "generic",
					"url": "https://cdn.filen.io/desktop/release/"
				}
			]
		},
		"mac": {
			"hardenedRuntime": true,
			"gatekeeperAssess": false,
			"entitlements": "./assets/entitlements.mac.plist",
			"entitlementsInherit": "./assets/entitlements.mac.plist",
			"category": "public.app-category.productivity",
			"darkModeSupport": true,
			"target": {
				"target": "default",
				"arch": [
					"x64",
					"arm64"
				]
			},
			"publish": [
				{
					"provider": "generic",
					"url": "https://cdn.filen.io/desktop/release/"
				}
			],
			"icon": "./assets/icons/mac/icon.icns"
		},
		"dmg": {
			"sign": false
		}
	}
}
